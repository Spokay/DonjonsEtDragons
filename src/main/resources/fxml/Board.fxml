<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.PerspectiveCamera?>
<?import javafx.scene.shape.Box?>

<?import javafx.scene.SubScene?>
<Group xmlns="http://javafx.com/javafx"
       xmlns:fx="http://javafx.com/fxml"
       fx:id="boardRoot">
    <Box depth="100" height="100" layoutX="0" layoutY="0" width="100" />
    <Box depth="100" height="100" layoutX="100" layoutY="100" width="100" />
    <Box depth="100" height="100" layoutX="200" layoutY="200" width="100" />
    <PerspectiveCamera fx:id="boardCamera" fieldOfView="-Infinity" layoutX="50.0" rotate="10.0">
    </PerspectiveCamera>
</Group>
